    "@angular/forms": "^8.1.3",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/platform-server": "^8.1.3",
    "@compodoc/gulp-compodoc": "^0.0.10",
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.14.13",
