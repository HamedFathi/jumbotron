    "gulp": "3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-conventional-changelog": "^2.0.19",
    "gulp-coveralls": "^0.1.4",
    "gulp-file": "^0.4.0",
    "gulp-git": "^2.7.0",
    "gulp-hub": "^0.8.0",
    "gulp-inline-ng2-template": "^4.1.0",
    "gulp-tslint": "^8.1.3",
    "html-loader": "^0.5.5",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jest-preset-angular": "^7.1.1",
